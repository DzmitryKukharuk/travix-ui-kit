{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "builder/**/*.js",
    "!builder/program.js",
    "!builder/webpack.config.js",
    "components/**/*.js",
    "!components/index.js"
  ],
  "maxWorkers": 2,
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "rootDir": "../",
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "transform": {
    ".*": "babel-jest"
  },
  "watchman": false
}
